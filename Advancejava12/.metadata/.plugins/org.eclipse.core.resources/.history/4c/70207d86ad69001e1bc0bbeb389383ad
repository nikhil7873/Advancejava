public class TestBundle {
      public static void main(String[] args) {
		
    	  ResourceBundle rb =  ResourceBundle.getBundle("in.co.rays.bundle.app_hi");
    	  
    	  String driver = rb.getString("greeting");
    	  
    	  System.out.println(driver);
	}

}
